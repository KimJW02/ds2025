int where(int x, int y, int h1, int h2, int v1, int v2) {
    if(y<v2 && h1<x && x<=h2) {
		return 1;
	}
    else if(y<v1 && h1<x && x<h2) {
    	return 1;
    }
    else if(y==v1 && h1<=x && x<=h2) {
    	return 2;
    }
    else if(x==h2 && v2<=y && y<=v1) {
    	return 2;
    }
    else {
    	return 0;
    }
};

int main(void){

    vector<int> h;
    vector<int> v;
    vector<int> v2;
    int sumv=0;

    while(true) {
        int a,b;
        cin>>a;

        if(a == 0) {
        	break;
        }
        else {
            cin>>b;
            h.push_back(a);
            v.push_back(b);
            sumv=sumv+b;
        }
    }
    v2.push_back(sumv);

    for(int i=1; i<h.size(); i++) {
    	h[i] = h[i-1]+h[i];
    }
    for(int i=0; i<v.size(); i++) {
    	v2.push_back(v2[i] - v[i]);
    }

    while(true) {
        int temp,p1,p2,res;
        res=0;
        cin>>p1>>p2;
        for(int i=0; i<h.size(); i++) {
            if(i==0) { 
            	temp = where(p1,p2,0,h[i],v2[i],v2[i+1]);
            }
            else if(i==v.size()-1) {
            	temp = where(p1,p2,h[i-1],h[i],v2[i],0);
            }
            else {
            	temp = where(p1,p2,h[i-1],h[i],v2[i],v2[i+1]);
            }

            if(temp != 0) {
            	res=temp;
            }
        }
        if(res == 1) {
        	cout<<"in\n";
        }
        else if(res==2) {
        	cout<<"on\n";
        }
        else if(res==0) {
        	cout<<"out\n";
        }
        if(cin.eof()) {
        	break;
        }
    }
    return 0;
}